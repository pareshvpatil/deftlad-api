{
  "name": "deftlad-api",
  "version": "1.0.2",
  "description": "Backend for Deftlad Website",
  "scripts": {
    "test": "echo 'no tests'",
    "clean": "rm -rf dist",
    "compile": "tsc --project tsconfig.compile.json",
    "build": "npm run clean && npm run barrels && npm run compile",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run start:local",
    "start:local": "node --require=ts-node/register src/index.ts",
    "start:server": "tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write"
  },
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@tsed/ajv": "^7.2.3",
    "@tsed/common": "^7.2.3",
    "@tsed/core": "^7.2.3",
    "@tsed/di": "^7.2.3",
    "@tsed/engines": "^7.2.3",
    "@tsed/exceptions": "^7.2.3",
    "@tsed/json-mapper": "^7.2.3",
    "@tsed/logger": "^6.3.2",
    "@tsed/logger-file": "^6.3.2",
    "@tsed/platform-cache": "^7.2.3",
    "@tsed/platform-exceptions": "^7.2.3",
    "@tsed/platform-express": "^7.2.3",
    "@tsed/platform-log-middleware": "^7.2.3",
    "@tsed/platform-middlewares": "^7.2.3",
    "@tsed/platform-params": "^7.2.3",
    "@tsed/platform-response-filter": "^7.2.3",
    "@tsed/platform-views": "^7.2.3",
    "@tsed/schema": "^7.2.3",
    "ajv": "^8.11.0",
    "axios": "1.1.3",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "neocities": "^0.0.3",
    "pcloud-sdk-js": "2.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.2.2",
    "@types/axios": "0.14.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  },
  "main": "lib/index.js",
  "author": "Deftlad",
  "license": "ISC",
  "tsed": {
    "packageManager": "npm",
    "convention": "default",
    "architecture": "default"
  }
}
